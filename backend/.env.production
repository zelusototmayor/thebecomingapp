# Production Environment - Copy this to .env on your DigitalOcean droplet

# Database password (change this!)
DB_PASSWORD=your-secure-database-password-here

# JWT Secrets (generate new ones for production!)
JWT_SECRET=generate-a-new-secret-with-openssl-rand-hex-32
JWT_REFRESH_SECRET=generate-another-secret-with-openssl-rand-hex-32

# Email (Resend)
RESEND_API_KEY=your-resend-api-key

# Frontend URL (your app's URL)
FRONTEND_URL=https://your-app-domain.com

# Node environment
NODE_ENV=production
